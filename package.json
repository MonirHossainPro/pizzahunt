{
  "name": "pizzahunt",
  "version": "1.0.0",
  "description": "Pizza Hunt MERN Project",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MonirHossainPro/pizzahunt.git"
  },
  "author": "Monir Hossain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MonirHossainPro/pizzahunt/issues"
  },
  "homepage": "https://github.com/MonirHossainPro/pizzahunt#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.12.3"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  }
}
